<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
    <action-mappings>
    <!-- 数据结算管理 -->
	  	 <action path="/databalance" type="com.pancou.ad.action.DataBalanceAction" parameter="action">
	  		<forward name="details" path="/WEB-INF/jsp/databalance/databalance_details.jsp"></forward>
	  		<forward name="list" path="/WEB-INF/jsp/databalance/databalance_list.jsp"></forward>
	  		<forward name="reportlist" path="/WEB-INF/jsp/databalance/datareport_list.jsp"></forward>
	  		<forward name="financeList" path="/WEB-INF/jsp/databalance/finance_list.jsp"></forward>
	  		<forward name="moneyReceiptState" path="/WEB-INF/jsp/databalance/datareport_money_receipt.jsp"></forward>
	  	</action>
	  	<action path="/databalancereport"  type="com.pancou.ad.action.DataBalanceReportAction"  >
	  	</action>
	  	
	  	<action path="/finance" type="com.pancou.ad.action.FinanceAction" parameter="action">
            <forward name="list" path="/WEB-INF/jsp/databalance/finance_list.jsp"></forward>
        </action>
	  	
	</action-mappings>
</struts-config>

