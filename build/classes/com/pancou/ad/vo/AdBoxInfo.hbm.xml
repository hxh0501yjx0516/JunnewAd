<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pancou.ad.vo.AdBoxInfo" table="AdBoxInfo" >
        <id name="adBoxInfoId" type="java.lang.Integer">
            <column name="AdBoxInfoId" />
            <generator class="identity"></generator>
        </id>
        <property name="adBoxInfoName" type="java.lang.String">
            <column name="AdBoxInfoName" length="50">
                <comment>广告位规格名称</comment>
            </column>
        </property>
        <property name="adSizeId" type="java.lang.Integer">
            <column name="AdSizeId" >
                <comment>广告位尺寸ID</comment>
            </column>
        </property>
         <property name="modelId" type="java.lang.Integer">
            <column name="ModelId" >
                <comment>广告位模式ID</comment>
            </column>
        </property>
         <property name="adBoxInfoRemarks" type="java.lang.String">
            <column name="AdBoxInfoRemarks" length="100">
                <comment>广告位备注</comment>
            </column>
        </property>
          <property name="adBoxInfoStatus" type="java.lang.Integer">
            <column name="AdBoxInfoStatus" >
                <comment>状态</comment>
            </column>
        </property>
        <property name="addTime" type="java.lang.String">
            <column name="AddTime" >
                <comment>添加时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
