<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pancou.ad.vo.Models" table="models" >
        <id name="modelId" type="java.lang.Integer">
            <column name="modelId" />
            <generator class="native"></generator>
        </id>
        <property name="modelName" type="java.lang.String">
            <column name="modelName" length="20">
                <comment>模板名称</comment>
            </column>
        </property>
        <property name="modelJs" type="java.lang.String">
            <column name="modelJs" length="20">
                <comment>模板文件</comment>
            </column>
        </property>
        <property name="modelStatus" type="java.lang.Integer">
            <column name="modelStatus" length="20">
                <comment>状态</comment>
            </column>
        </property>
          <property name="modelFlag" type="java.lang.Integer">
            <column name="ModelFlag" >
                <comment>类型</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
